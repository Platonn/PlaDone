- model_class = ProjectLogEntry
.page-header
  %h1=t '.title', :default => model_class.model_name.human.titleize

%p
  %strong= model_class.human_attribute_name(:project_id) + ':'
  %br
  = @project_log_entry.project_id
%p
  %strong= model_class.human_attribute_name(:user_id) + ':'
  %br
  = @project_log_entry.user_id
%p
  %strong= model_class.human_attribute_name(:activity_type) + ':'
  %br
  = @project_log_entry.activity_type
%p
  %strong= model_class.human_attribute_name(:task_id) + ':'
  %br
  = @project_log_entry.task_id
%p
  %strong= model_class.human_attribute_name(:previous_value) + ':'
  %br
  = @project_log_entry.previous_value
%p
  %strong= model_class.human_attribute_name(:new_value) + ':'
  %br
  = @project_log_entry.new_value


= link_to t('.back', :default => t("helpers.links.back")), project_log_entries_path, :class => 'btn btn-default'
= link_to t('.edit', :default => t("helpers.links.edit")), edit_project_log_entry_path(@project_log_entry), :class => 'btn btn-default'
= link_to t('.destroy', :default => t("helpers.links.destroy")), project_log_entry_path(@project_log_entry), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
